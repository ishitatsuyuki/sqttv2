  },
  "devDependencies": {
    "@bufbuild/protoc-gen-connect-web": "^0.3.2",
    "@bufbuild/protoc-gen-es": "^0.3.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vue-tsc": "^1.0.9"
  }
